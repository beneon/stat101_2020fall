# Title     : var x + y
# Objective : demonstrate var(x+y) = var(x)+var(y)+2cov(x,y)
# Created by: Administrator
# Created on: 2020/10/11
library(ggplot2)
library(dplyr)
n = 50
x = rnorm(n,10,2)
y = rnorm(n,10,2)
var_x = var(x)
var_y = var(y)
var_x_y = var(x+y)
cov_x_y = cov(x,y)
